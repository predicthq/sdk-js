{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Place Search Schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "q": {
      "type": "string"
    },
    "id": {
      "type": "array"
    },
    "country": {
      "type": "array"
    },
    "limit": {
      "type": "integer",
      "minimum": 1,
      "maximum": 200
    },
    "location": {
      "type": "string",
      "pattern": "@([\\-\\+]?\\d+(\\.\\d+)?),([\\-\\+]?\\d+(\\.\\d+)?)"
    },
    "type": {
      "items": {
        "enum": [
          "neighbourhood",
          "locality",
          "localadmin",
          "county",
          "region",
          "country",
          "continent",
          "country",
          "planet",
          "local",
          "metro",
          "major"
        ]
      },
      "type": "array"
    }
  },
  "required": [

  ]
}